	<skos:definition xml:lang="fr">Lieu</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-017"/>
	<skos:prefLabel xml:lang="fr">en musée</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Désigne tout type de musée. La ressource est utilisée en complément ou en illustration des expositions du musée. S'applique à une ressource dont le contexte d'utilisation est un musée.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-017"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/scolomfr-voc-017-num-0011">
