	<skosxl:prefLabel rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-041-num-033"/>
	<canope:labelEquivalentExact xml:lang="fr">trouble du psychisme</canope:labelEquivalentExact>
	<canope:labelEquivalentExact xml:lang="fr">troubles psychiques</canope:labelEquivalentExact>
	<canope:termeEquivalentExact rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-041-num-008"/>
	<canope:termeEquivalentExact rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-041-num-043"/>
</rdf:Description>

