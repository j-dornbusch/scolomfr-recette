	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">éditeur de publication</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Entité responsable de la publication qu'elle soit papier ou numérique. Ne doit pas être confondu avec « éditeur de contenu » et « directeur d'ouvrage ».</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/scolomfr-voc-003-num-030">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">directeur d'ouvrage</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Entité qui rassemble les éditeurs de contenus sous sa direction et qui coordonne leur travail. Ne doit pas être confondu avec « éditeur de contenu » et « éditeur de publication ».</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/scolomfr-voc-003-num-031">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">illustrateur</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Entité qui a réalisé tout ou partie des illustrations de la ressource.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/script_writer">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">rédacteur de scénario</skos:altLabel>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">rédacteur de scénario</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Entité responsable de la création du déroulement d'une action ou de la trame pédagogique d'une ressource. On utilisera "auteur" si le rédacteur de scénario est le principal responsable du contenu d'une ressource. Concept issu du LOMFR.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<canope:labelEquivalentExact xml:lang="fr">rédacteur de scénario</canope:labelEquivalentExact>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/subject_matter_expert">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">expert du domaine</skos:altLabel>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">expert du domaine</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Entité qualifiée pour juger de la pertinence du contenu de la ressource. Dans le cas où un expert du domaine fournit l'information, il est alors également "fournisseur de contenu". Concept issu du LOMFR.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<canope:labelEquivalentExact xml:lang="fr">expert du domaine</canope:labelEquivalentExact>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/technical_implementer">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">développeur informatique</skos:altLabel>
	<skos:altLabel xml:lang="fr">implémenteur technique</skos:altLabel>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">implémenteur technique</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Entité qui effectue l'ensemble des opérations techniques nécessaires pour élaborer la ressource. Par exemple : une cellule TICE, un développeur informatique... Concept issu du LOMFR.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<canope:labelEquivalentExact xml:lang="fr">développeur informatique</canope:labelEquivalentExact>
	<canope:labelEquivalentExact xml:lang="fr">implémenteur technique</canope:labelEquivalentExact>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/technical_validator">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">valideur technique</skos:altLabel>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">valideur technique</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">"Entité responsable de confirmer l'intégrité pédagogique de la ressource" (Source : Normetic v1.1). Concept issu du LOMFR.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<canope:labelEquivalentExact xml:lang="fr">valideur technique</canope:labelEquivalentExact>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/terminator">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">terminateur</skos:altLabel>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">terminateur</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">Entité qui peut refuser la diffusion de la ressource. Concept issu du LOMFR.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<canope:labelEquivalentExact xml:lang="fr">terminateur</canope:labelEquivalentExact>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/unknown">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">inconnu</skos:altLabel>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">inconnu</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">"Utiliser ce terme pour toute entité dont la contribution ou le rôle est incertain" (Source : Normetic v1.1). Lui préférer "contributeur". Concept issu du LOMFR.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<canope:labelEquivalentExact xml:lang="fr">inconnu</canope:labelEquivalentExact>
</rdf:Description>

<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/validator_2">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">valideur</skos:altLabel>
	<skos:altLabel xml:lang="fr">valideur juridique</skos:altLabel>
	<skos:definition xml:lang="fr">Rôle (contributeur)</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<skos:prefLabel xml:lang="fr">valideur</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">"Entité responsable de confirmer l'intégrité de la ressource" (Source : Normetic v1.1). S'emploie aussi pour d'autres types de valideur comme un valideur juridique. Concept issu du LOMFR.</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-003"/>
	<canope:labelEquivalentExact xml:lang="fr">valideur</canope:labelEquivalentExact>
	<canope:labelEquivalentExact xml:lang="fr">valideur juridique</canope:labelEquivalentExact>
</rdf:Description>

