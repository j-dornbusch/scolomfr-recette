	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-040"/>
	<skos:prefLabel xml:lang="fr">éducation artistique et culturelle</skos:prefLabel>
	<skos:related rdf:resource="http://data.education.fr/voc/scolomfr/concept/scolomfr-voc-040-num-022"/>
	<skos:related rdf:resource="http://data.education.fr/voc/scolomfr/concept/scolomfr-voc-040-num-030"/>
	<skos:scopeNote xml:lang="fr">L'éducation artistique et culturelle s'appuie essentiellement sur les enseignements obligatoires et optionnels, les classes à PAC et les activités facultatives. [Eduscol]</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-040"/>
