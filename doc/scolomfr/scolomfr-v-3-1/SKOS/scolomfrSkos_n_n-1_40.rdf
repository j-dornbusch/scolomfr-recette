<rdf:Description rdf:about="http://data.education.fr/voc/scolomfr/concept/scolomfr-voc-040-num-023">
	<isothes:status xml:lang="fr">Actuel</isothes:status>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
