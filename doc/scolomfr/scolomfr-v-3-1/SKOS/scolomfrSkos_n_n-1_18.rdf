	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="fr">e-learning</skos:altLabel>
	<skos:altLabel xml:lang="fr">formation en ligne</skos:altLabel>
	<skos:definition xml:lang="fr">Modalité pédagogique</skos:definition>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr"/>
	<skos:inScheme rdf:resource="http://data.education.fr/voc/scolomfr/scolomfr-voc-018"/>
	<skos:prefLabel xml:lang="fr">en ligne</skos:prefLabel>
	<skos:scopeNote xml:lang="fr">S'applique à une ressource qui se destine à un usage « en ligne », c'est-à-dire par le biais d'une connexion à internet (rédaction d'un mail, participation à un réseau social, recherche d'information sur le web, jeu ou exercice  « en ligne », etc).</skos:scopeNote>
	<skos:topConceptOf rdf:resource="http://data.education.fr/voc/scolomfr"/>
